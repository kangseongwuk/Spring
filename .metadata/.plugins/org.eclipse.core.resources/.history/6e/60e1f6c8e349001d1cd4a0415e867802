package kr.co.ezen.beans;

public class TestBean {

	private int data1; //필드변수는 자동 초기화가 됨 0 => 10으로 초기화
	private double data2; // 0.0 => 10.0
	private String data3; // null => null
	
	
	/*
	 * public TestBean() { //default Constructor 디폴트 생성자
	 * System.out.println("기본생성자 입니다.");
	 * 
	 * this.data1 = 10; this.data2 = 10.0; this.data3 = null; }
	 */
	public TestBean(int x, double y, String w) {
		// 생성자 오버로딩
		
		this.data1 = x;
		this.data2 = y;
		this.data3 = w;
	}
	public void printData() {
		System.out.println("data1 : " + data1);
		System.out.println("data2 : " + data2);
		System.out.println("data3 : " + data3);
	}

	public int getData1() {
		return data1;
	}

	public void setData1(int data1) {
		this.data1 = data1;
	}

	public double getData2() {
		return data2;
	}

	public void setData2(double data2) {
		this.data2 = data2;
	}

	public String getData3() {
		return data3;
	}

	public void setData3(String data4) {
		this.data3 = data4;
	}


	
}
